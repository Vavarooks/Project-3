    "jwt-simple": "^0.5.6",
    "mongoose": "^5.9.18",
    "mysql": "^2.17.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "plotly.js": "^1.54.2",
    "react-plotly.js": "^2.4.0",
    "react-sound": "^1.2.0",
